package {	import flash.display.MovieClip;	import flash.display.SimpleButton;	import flash.events.MouseEvent;	import flash.text.TextField;	import flash.utils.Dictionary;		/**	 * @author colin	 */	public class Main extends MovieClip{		public var btnAction:SimpleButton;		public var txtCmd:TextField;		public var txtScript:TextField;				private var commands:Dictionary;				public function Main(){			txtCmd.addEventListener(MouseEvent.CLICK, clear);			btnAction.addEventListener(MouseEvent.CLICK,exec);						commands = new Dictionary();			commands['scaffold'] = new Scaffold();		}				private function clear(e:MouseEvent):void{			txtCmd.text = "";			txtCmd.removeEventListener(MouseEvent.CLICK, clear);		}				private function exec(e:MouseEvent):void{			//scaffold Class:Parent[DisplayObject,Goo] -rw name:String='hi' -r sex:String -w writeOnly:Boolean  			var proc:Array = txtCmd.text.split(" ");			if(commands[proc[0]]==null)return;						txtScript.text = commands[proc.shift()].exec(proc.join(" "));		}	}}